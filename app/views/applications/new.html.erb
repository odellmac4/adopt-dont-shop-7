<%= form_with url: "/applications?status=inprogress", method: :post  do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :street_address %>
    <%= f.text_field :street_address %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :zip_code %>
    <%= f.text_field :zip_code %>

    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
  <div>
    <%= f.submit 'Submit' %>
  </div>
<% end %>